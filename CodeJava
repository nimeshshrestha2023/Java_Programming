class Main{
    public static void main(String[] args)
    {
        String str = "Nimesh Shrestha";
        String str1 = "";
        int l,i;
        
        l = str.length();
        for(i=l-1;i>=0;i--)
        {
            str1 = str1+str.charAt(i);
        }
        System.out.println(str1);
    }
